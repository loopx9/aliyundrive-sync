		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.1.1",
		"eslint-plugin-prettier": "^5.2.6",
		"obsidian": "latest",
		"prettier": "^3.5.3",
